# def hap(n):
#     sum_ = 0
#     for k in range(1, n+1):
#         sum_ += k
#     return sum_

n = int(input())
l = []
l1 = []
for i in range(n):
    a = int(input())
    l.append(a)

for j in range(n-1):
    l[j] + l[j+1]

